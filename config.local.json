{
  "app": {
    "name": "boilerplate",
    "port": 8080,
    "limit": 20,
    "page": 1,
    "sort": "created_at desc",
    "userCreateAuthSkip": false,
    "UserConfirmationPrefix": "UserActivation_",
    "mockPasswordEnabled": true,
    "mockPassword": "12345678",
    "SendEmail": false
  },
  "db": {
    "host": "boilerplate_mysql",
    "port": "3306",
    "user": "boilerplate_user",
    "pass": "12345678",
    "schema": "boilerplate",
    "maxIdleConn": 1,
    "maxOpenConn": 2,
    "maxConnLifetime": 30,
    "debug": true
  },
  "jwt": {
    "accessTokenSecret": "accesstokensecret",
    "refreshTokenSecret": "refreshtokensecret",
    "accessTokenExpiry": 3000,
    "refreshTokenExpiry": 10080,
    "contextKey": "user"
  },
  "mail": {
    "sender": "boilerplate Admin <admin@boilerplate.xyz>",
    "passwordReseturl": "http://localhost:8081/password/reset",
    "welcomeMessage": "We're very excited to have you on board",
    "passwordMessage": "The password is auto generated, please change immediately",
    "mailgun": {
      "apiKey": "put mail-api-key",
      "domain": "mail.domain.xyz",
      "subject": {
        "userCreated": "User Created",
        "forgotPassword": "Forgot Password",
        "passwordReset": "Password Reset"
      },
      "templateName": {
        "enterpriseUserCreate": "welcome",
        "forgotPassword": "password_reset"
      }
    }
  }
}